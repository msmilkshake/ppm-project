<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefWidth="330.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.LoadPopup">
   <children>
      <VBox layoutX="103.0" layoutY="55.0" prefWidth="100.0" spacing="12.0" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="40.0" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="30.0">
         <children>
            <Label text="Enter the save filename (without file extension)" />
            <Label fx:id="lblInvalid" text="File does not exist" textFill="#d71111">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="txtSave">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin></TextField>
            <HBox prefWidth="200.0" spacing="20.0">
               <children>
                  <Button fx:id="btnLoad" mnemonicParsing="false" onMouseClicked="#btnLoadOnClicked" prefWidth="60.0" text="Load">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin></Button>
                  <Button fx:id="btnCancel" mnemonicParsing="false" onMouseClicked="#btnCancelOnClicked" prefWidth="60.0" text="Cancel" />
               </children>
            </HBox>
         </children></VBox>
   </children>
</AnchorPane>
